{
    "name": "vanbrabantf/npm-stat-fetcher",
    "description": "A small package that returns some common stats about npm packages",
    "type": "library",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Vanbrabantf\\NpmStatFetcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanbrabantf\\NpmStatFetcher\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Frederick Vanbrabant",
            "email": "frederick.vanbrabant@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
